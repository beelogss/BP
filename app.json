{
  "expo": {
    "name": "Bottle Points",
    "slug": "BP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/square-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/square-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "b.pts_bcltv",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "@react-native-firebase/app",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8b1bf633-da5f-4e6f-bc51-96aeee7f18e8"
      }
    }
  }
}
